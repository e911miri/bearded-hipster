<h1>Listing doctorspatients</h1>

<table>
  <tr>
    <th>Doctor</th>
    <th>Patient</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @doctorspatients.each do |doctorspatient| %>
  <tr>
    <td><%= doctorspatient.doctor_id %></td>
    <td><%= doctorspatient.patient_id %></td>
    <td><%= link_to 'Show', doctorspatient %></td>
    <td><%= link_to 'Edit', edit_doctorspatient_path(doctorspatient) %></td>
    <td><%= link_to 'Destroy', doctorspatient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

